<%= form_for @festival do |f| %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :start_date %>
  <%= f.text_field :start_date %>

  <%= f.label :end_date %>
  <%= f.text_field :end_date %>

  <%= f.label :main_headliner %>
  <%= f.text_field :main_headliner %>

  <%= f.label :ticket_price %>
  <%= f.text_field :ticket_price %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit 'Create festival!' %>

<% end %>